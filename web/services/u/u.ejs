                        <script>
                        mapboxgl.accessToken = 'pk.eyJ1IjoieXJhZWxsZSIsImEiOiJjazNpZTRvNHAwNTczM21wbGt6bGxoZTVoIn0.8WCCVP2e-D6vcRmglRk_ag';
